                            </xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="binaryType" use="optional">
                            <xs:simpleType>
                                <xs:restriction base="xs:normalizedString">
                                    <xs:enumeration value="x86-64-sol10"/>
                                    <xs:enumeration value="x86-64-sol10-gcc3.4.3"/>

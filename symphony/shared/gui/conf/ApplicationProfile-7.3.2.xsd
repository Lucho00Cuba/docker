                            </xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="postExecCmd" type="xs:string" use="optional">
                          <xs:annotation>
                            <xs:documentation>
                              Obsolete in Symphony 4.0.
                            </xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>

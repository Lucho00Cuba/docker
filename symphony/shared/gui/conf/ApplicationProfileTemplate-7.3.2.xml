<?xml version="1.0" encoding="UTF-8"?>
<Profile xmlns="http://www.platform.com/Symphony/Profile/Application" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="" version="7.3.2">
	<Consumer consumerId="" resReq="select(!mg)" applicationName="" numOfSlotsForPreloadedServices="1" taskHighWaterMark="1.0" taskLowWaterMark="1.0" preStartApplication="false"/>
	<SOAM version="7.3.2">
		<SSM resReq="" startUpTimeout="60" shutDownTimeout="300" workDir="${EGO_SHARED_TOP}/soam/work">
			<boundaryManagerConfig>
				<boundaries>
					<boundary elementName="AvailableMemory">
						<event name="BEV_PROACTIVE" value="50"/>
						<event name="BEV_SEVERE" value="40"/>
						<event name="BEV_CRITICAL" value="25"/>
						<event name="BEV_HALT" value="15"/>
					</boundary>
					<boundary elementName="ProcessMemory">
						<!-- MaxSizeReference will be capped at "2048" by ssm when it runs on 32 bit OS -->
						<param name="MaxSizeReference" value="8388608"/>
						<event name="BEV_PROACTIVE" value="0"/>
						<event name="BEV_SEVERE" value="0"/>
						<event name="BEV_CRITICAL" value="0"/>
						<event name="BEV_HALT" value="0"/>
						</boundary>
					<boundary elementName="AvailableVirtualAddressSpace">
						<!-- MaxSizeReference will be capped at "2048" by ssm when it runs on 32 bit OS -->
						<param name="MaxSizeReference" value="8388608"/>
						<event name="BEV_PROACTIVE" value="50"/>
						<event name="BEV_SEVERE" value="40"/>
						<event name="BEV_CRITICAL" value="25"/>
						<event name="BEV_HALT" value="15"/>
					</boundary>
				</boundaries>
			</boundaryManagerConfig>
		</SSM>		
		<SIM startUpTimeout="120" blockHostOnTimeout="true" blockHostOnVersionMismatch="true"/>
		<DataHistory pollFrequency="10" fileSwitchTime="24" fileSwitchSize="100" lastingPeriod="96" path="history"/>
		<PagingTasksInput blockSize="4096" diskSpace="4294967296"/>
		<PagingTasksOutput blockSize="4096" diskSpace="4294967296"/>
		<PagingCommonData blockSize="102400" diskSpace="8589934592"/>
		<PagingCommonDataUpdates blockSize="102400" diskSpace="8589934592"/>
	</SOAM>
	<SessionTypes>
		<Type name="" priority="1" recoverable="false" sessionRetryLimit="3" taskRetryLimit="3" abortSessionIfTaskFail="false" abortSessionIfClientDisconnect="true" suspendGracePeriod="100" taskCleanupPeriod="250"/>
	</SessionTypes>
	<Service default="true" name="">
		<osTypes>
			<osType startCmd="" name=""/>
		</osTypes>
		<Control>
			<Method name="Register">
				<Timeout duration="60" actionOnSI="blockHost" retryLimit="0" retryInterval="0" customizedDebugAction="writeServiceEventReplayFiles"/>
				<Exit actionOnSI="blockHost" retryLimit="0" retryInterval="0" customizedDebugAction="writeServiceEventReplayFiles"/>
			</Method>
			<Method name="CreateService">
				<Timeout duration="0" actionOnSI="blockHost" customizedDebugAction="writeServiceEventReplayFiles"/>
				<Exit actionOnSI="blockHost" customizedDebugAction="writeServiceEventReplayFiles"/>
				<Return controlCode="0" actionOnSI="keepAlive" customizedDebugAction="none"/>
				<Exception type="failure" controlCode="0" actionOnSI="blockHost" customizedDebugAction="writeServiceEventReplayFiles"/>
				<Exception type="fatal" controlCode="0" actionOnSI="blockHost" customizedDebugAction="writeServiceEventReplayFiles"/>
			</Method>
			<Method name="SessionEnter">
				<Timeout duration="0" actionOnSI="blockHost" actionOnWorkload="retry" customizedDebugAction="writeServiceEventReplayFiles"/>
				<Exit actionOnSI="blockHost" actionOnWorkload="retry" customizedDebugAction="writeServiceEventReplayFiles"/>
				<Return controlCode="0" actionOnSI="keepAlive" actionOnWorkload="succeed" customizedDebugAction="none"/>
				<Exception type="failure" controlCode="0" actionOnSI="keepAlive" actionOnWorkload="retry" customizedDebugAction="writeServiceEventReplayFiles"/>
				<Exception type="fatal" controlCode="0" actionOnSI="keepAlive" actionOnWorkload="fail" customizedDebugAction="writeServiceEventReplayFiles"/>
			</Method>
			<Method name="SessionUpdate">
				<Timeout duration="0" actionOnSI="blockHost" actionOnWorkload="retry" customizedDebugAction="writeServiceEventReplayFiles"/>
				<Exit actionOnSI="blockHost" actionOnWorkload="retry" customizedDebugAction="writeServiceEventReplayFiles"/>
				<Return controlCode="0" actionOnSI="keepAlive" actionOnWorkload="succeed" customizedDebugAction="none"/>
				<Exception type="failure" controlCode="0" actionOnSI="keepAlive" actionOnWorkload="retry" customizedDebugAction="writeServiceEventReplayFiles"/>
				<Exception type="fatal" controlCode="0" actionOnSI="keepAlive" actionOnWorkload="fail" customizedDebugAction="writeServiceEventReplayFiles"/>
			</Method>
			<Method name="Invoke">
				<Timeout duration="0" actionOnSI="restartService" actionOnWorkload="retry" customizedDebugAction="writeServiceEventReplayFiles"/>
				<Exit actionOnSI="restartService" actionOnWorkload="retry" customizedDebugAction="writeServiceEventReplayFiles"/>
				<Return controlCode="0" actionOnSI="keepAlive" actionOnWorkload="succeed" customizedDebugAction="none"/>
				<Exception type="failure" controlCode="0" actionOnSI="keepAlive" actionOnWorkload="retry" customizedDebugAction="writeServiceEventReplayFiles"/>
				<Exception type="fatal" controlCode="0" actionOnSI="keepAlive" actionOnWorkload="fail" customizedDebugAction="writeServiceEventReplayFiles"/>
			</Method>
			<Method name="SessionLeave">
				<Timeout duration="0" actionOnSI="restartService" customizedDebugAction="writeServiceEventReplayFiles"/>
				<Exit actionOnSI="restartService" customizedDebugAction="writeServiceEventReplayFiles"/>
				<Return controlCode="0" actionOnSI="keepAlive" customizedDebugAction="none"/>
				<Exception type="failure" controlCode="0" actionOnSI="keepAlive" customizedDebugAction="writeServiceEventReplayFiles"/>
				<Exception type="fatal" controlCode="0" actionOnSI="keepAlive" customizedDebugAction="writeServiceEventReplayFiles"/>
			</Method>
			<Method name="DestroyService">
				<Timeout duration="15" customizedDebugAction="writeServiceEventReplayFiles"/>
			</Method>
			<Method name="Deployment" retryLimit="0" retryInterval="0">
				<Timeout duration="0"/>
			</Method>
			<Method name="Startup" retryLimit="0" retryInterval="0"/>
		</Control>
	</Service>
</Profile>

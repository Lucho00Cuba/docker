<?xml version="1.0" encoding="UTF-8"?> 
<!DOCTYPE ParamConfs SYSTEM "pmc_conf.dtd"> 
<ParamConfs> 
	<MainConsole>
		<Module Name="Resources" UserAclName="Main_host" URL="/platform/getHostInfo.controller" OnlineHelpURL ="/doc/help/host_information_console.html" IconStyle="moduleTab-2">
			<ModuleItem Name="hosts" UserAclName="Main_host" URL="/platform/getHostInfo.controller" HasConsumerTree="yes" OnlineHelpURL ="/doc/help/host_information_console.html"/>
			<ModuleItem Name="rMonitor" UserAclName="Consumer_plan_allocationResource" URL="/platform/getResourceAllocation.controller" HasConsumerTree="yes" OnlineHelpURL ="/doc/help/resource_allocation_view.html"/>
			<ModuleItem Name="rgroup" UserAclName="Main_rGroup" URL="/platform/getResourceGroup.controller" HasConsumerTree="no" OnlineHelpURL ="/doc/help/resource_groups.html"/>
			<ModuleItem Name="rsregion" UserAclName="Main_RS_Region" URL="/platform/getRegionList.controller" HasConsumerTree="no" OnlineHelpURL ="/doc/help/repository_service_regions.html"/>

		</Module>
        <Module Name="Consumers" UserAclName="Main_consumer_plan" URL="/platform/ConsumerTree.controller" OnlineHelpURL ="/doc/help/tree_building.html" IconStyle="moduleTab-3">
			<ModuleItem Name="consumer_plan" UserAclName="Main_consumer_plan" URL="/platform/ConsumerTree.controller" HasConsumerTree="yes" OnlineHelpURL ="/doc/help/tree_building.html"/>
			<ModuleItem Name="rMonitor" UserAclName="Consumer_plan_allocationResource" URL="/platform/getResourceAllocation.controller" HasConsumerTree="yes" OnlineHelpURL ="/doc/help/resource_allocation_view.html"/>
		</Module>
		<Module Name="Service" UserAclName="Main_service" URL="/platform/getServiceList.controller" OnlineHelpURL ="/doc/help/control_services.html" IconStyle="moduleTab-5">
			<ModuleItem Name="workload" UserAclName="Main_service" URL="/platform/getServiceList.controller" HasConsumerTree="yes" OnlineHelpURL="/doc/help/control_services.html"/>
			<ModuleItem Name="configuration" UserAclName="Main_service" URL="/platform/getServiceConfigureList.controller" HasConsumerTree="yes" OnlineHelpURL="/doc/help/control_services.html"/>
		</Module>
		<Module Name="EGO" UserAclName="Main_EGO" URL="/platform/getUserInfo.controller" OnlineHelpURL ="/doc/help/user_accounts.html" LogoutURL="/platform/logout.controller" IconStyle="moduleTab-1">
			<ModuleItem Name="user" UserAclName="Main_user" URL="/platform/getUserInfo.controller" OnlineHelpURL ="/doc/help/user_accounts.html"/>
			<ModuleItem Name="userRole" UserAclName="Main_user_role" URL="/platform/getRoleInfo.controller" OnlineHelpURL ="/doc/help/configurable_roles.html"/>
			<ModuleItem Name="clusterSetting" UserAclName="Main_clusterSetting" URL="/platform/getClusterProp.controller" HasConsumerTree="no"  OnlineHelpURL ="/doc/help/cluster_settings_summary.html"/>
		</Module>
        <Module Name="LOG" UserAclName="Main_logretrieve" URL="/platform/logManageAction.controller" OnlineHelpURL ="/doc/help/cluster_settings_summary.html" IconStyle="moduleTab-4">
			<ModuleItem Name="StandardLog" UserAclName="Main_logretrieve" URL="/platform/logManageAction.controller" HasConsumerTree="no"  OnlineHelpURL ="/doc/help/cluster_settings_summary.html"/>
		</Module>
		<Module Name="PERF" UserAclName="Main_PERF" OnlineHelpURL ="/doc/help/standard_reports.html"   URL="/perfgui/reportListingAction" LogoutURL="/perfgui/logout.controller" IconStyle="moduleTab-8">
			<ModuleItem Name="standardReport" OnlineHelpURL ="/doc/help/standard_reports.html" UserAclName="Main_perf_standard" URL="/perfgui/reportListingAction.controller?reportType=2" />
			<ModuleItem Name="customReport" OnlineHelpURL ="/doc/help/custom_reports.html"  UserAclName="Main_perf_custom" URL="/perfgui/reportListingAction.controller?reportType=1" />
		</Module>
        
		<Module Name="HARVESTING" UserAclName="Main_HARVESTING" URL="/platform/getHarvestingHostInfo.controller" OnlineHelpURL="/doc/help/scav_hosts_list_understanding.html" IconStyle="moduleTab-9">
			<ModuleItem Name="harvestingHost" UserAclName="Main_harvesting_host" URL="/platform/getHarvestingHostInfo.controller" />
			<ModuleItem Name="harvestingConf" UserAclName="Main_harvesting_conf" URL="/platform/getHarvestingConfiguration.controller" />
		</Module>
       
	</MainConsole>
	<Configuration>
		<Parameter>
			<Name>EGOHostListMaxRowPerPage</Name> 
			<Value>100</Value>
		</Parameter>
		<Parameter>
			<Name>EGOServiceInstanceMaxRowPerPage</Name> 
			<Value>100</Value>
		</Parameter>
		<Parameter>
			<Name>hostTooltipsFlag</Name> 
			<!-- this value are "true" or "false", "true" is show Tooltip, belong to EGO-->
			<Value>true</Value>
		</Parameter>
		<Parameter> 
			<Name>HostControlFlag</Name> 
			<!-- this value are "yes" or "no", "yes" is to show "Start and Shutdown"-->
			<Value>no</Value> 
		</Parameter>
		<Parameter>
			<Name>SessionExpireTime</Name>
			<Value>60</Value>
		</Parameter>
		<Parameter>
			<Name>HostChartInterval</Name>
			<Value>10</Value>
		</Parameter>
		<Parameter>
			<Name>userDefaultOption</Name> 
			<!-- this value is user default option(forward page) after user logon, when user is only consumer user , belong to EGO-->
			<Value>EGO</Value>
		</Parameter>
		<Parameter>
			<Name>userDefaultOptionItem</Name> 
			<!-- this value is user default option item(forward page) after user logon, when user is only consumer user , belong to EGO-->
			<Value>hosts</Value>
		</Parameter>
		<Parameter>
			<Name>adminDefaultOption</Name> 
			<!-- this value is user default option(forward page) after user logon, when user is consumer admin , belong to EGO-->
			<Value>EGO</Value>
		</Parameter>
		<Parameter>
			<Name>adminDefaultOptionItem</Name> 
			<!-- this value is user default option item(forward page) after user logon, when user is consumer admin , belong to EGO-->
			<Value>consumer_plan</Value>
		</Parameter>
		<Parameter>
			<Name>protectedResourceGroup</Name>
			<!-- this value list of service names whose action is prevented on GUI , belong to EGO-->
			<Value>InternalResourceGroup;ManagementHosts;MapReduceInternalResourceGroup;</Value>
		</Parameter>
		<Parameter>
			<Name>hideInternalResourceGroup</Name>
			<!-- This parameter allows the user to define which resource group should be hidden on consumer properties page. -->
			<!-- By default, not define this parameter, it will hide InternalResourceGroup, MapReduceInternalResourceGroup, DataNodeRG, -->
			<!-- NameNodeRG and SecondaryNodeRG internal resource groups. Resource group names must be separated by a semicolon (";"). -->
			<Value>InternalResourceGroup;MapReduceInternalResourceGroup;DataNodeRG;NameNodeRG;SecondaryNodeRG;</Value>
		</Parameter>		
		<Parameter>
			<Name>NumAccesses</Name> 
			<!-- This value indicate max num accesses per user, belong to EGO-->
			<Value>50</Value>
		</Parameter>
		<Parameter>
			<!--  Log retrieve resource number limit -->
			<Name>MaxResouceCapNum</Name>    	
			<Value>200</Value>
		</Parameter>
		<Parameter>
			<!--  Consumer for Log retrieve -->
			<Name>EGOSystemConsumer</Name>    	
			<Value>/ClusterServices/EGOClusterServices</Value>
		</Parameter>
		<Parameter>
			<!--  the reporting framework reporting data source name -->
			<Name>DataSourceName</Name>       
			<Value>ReportDB</Value>
		</Parameter>
		<Parameter>
			<!--  Maximum row number allowed to be export for the reporting framework report -->
			<Name>MaxRowNum</Name>       
			<Value>10000</Value>
		</Parameter>
		<Parameter>
			<Name>EnableROAlias</Name>
			<!--Specifies that read-only users can view and query reports from the cluster management console. Enable this parameter to true to allow users to query data in standard reports. --> 
			<!--This parameter is only applicable if you are using Oracle as your database. -->
			<!--Valid values are true or false. By default, this parameter is false. -->
			<Value>false</Value>
		</Parameter>
		<Parameter>
			<Name>SchemaOwner</Name>
			<!--When the EnableROAlias parameter is true, and the schema owner is not the same as the ReportDB user, specify the name of the schema owner as the value for this parameter.-->
			<!--This parameter value is case-sensitive. By default, this parameter is null.-->
			<Value></Value>
		</Parameter>
		<Parameter>
			<!--  Cluster policy parameter -->
			<Name>ClusterPolicyPara</Name>       
			<Value>ShareReclaim:Enable;ReclaimBeforeBorrow:Disable</Value>
		</Parameter>
		<Parameter>
			<!-- this value are "true" or "false", "true" is show "Enable Hybrid" in resource plan page -->
			<Name>isShowHybrid</Name>
			<Value>true</Value>
		</Parameter>
		<Parameter>
			<!--Enable the feature to manage resource plan on a big cluster.  -->
			<!--Valid values are Yes or No (case insensitive). By default, this parameter is No. -->
			<Name>EnhancedResourcePlanner</Name>
			<Value>No</Value>
		</Parameter>
		<Parameter>
			<!--  log retrieve connection timeout -->
			<Name>ConnTimeout</Name>       
			<Value>15</Value>
		</Parameter>
		<Parameter>
			<!--  log retrieve response timeout -->
			<Name>ResTimeout</Name>       
			<Value>60</Value>
		</Parameter>
		<Parameter>
			<Name>HideLogRetrieval</Name> 
			<!--Parameter of EGO: The value is either "true" or "false", any other value is treated as "false" as defalt; "true" is to hide log retrieval feature-->
			<Value>false</Value>
		</Parameter>
		<Parameter>
			<Name>RefreshCredential</Name> 
			<!-- refresh credential every this value, unit minute-->
			<Value>120</Value>
		</Parameter>
		<Parameter>
			<Name>CacheRefreshInterval</Name> 
			<!-- The CacheRefreshInterval defines the default cache refresh interval for all management console Cache. 
			The specified interval must be expressed in a whole number of seconds. If the interval is 0 
			or less, management console ensures the default behavior of cache is disabled. If this interval is not 
			specified, management console will set its default interval 5s.-->
			<Value>5</Value>
		</Parameter>
		<!-- <Parameter>
			<Name>ManagementResourceGroup</Name> 
			This parameter allows the user to define which resource groups should be included in the management allocation display. 
			Replace "G1" and "G2" with the resource groups that need to be counted as management allocations. 
			There is no limit to the number of resource groups that can be included. Resource group names must be separated by a semicolon (";").
			Note: Only the management console display is affected by this parameter. No other changes are made to the cluster.
			<Value>G1;G2</Value>
		</Parameter> -->	
		<Parameter>
			<Name>UserListThreshold</Name> 
			<!--Defines the maximum number of users that can be displayed on a management console user list. The default value is 5000. If a value greater than 5000 is defined, the management console may hang. -->
			<Value>5000</Value>
        </Parameter>
		<Parameter>
			<Name>UserMonitorTimely</Name> 
			<!--The value is either "true" or "false", defines whether monitoring the users login and logout message. The default value is false. -->
			<Value>false</Value>
		</Parameter>	
		<Parameter>
			<Name>RESTProxy</Name> 
			<Value>true</Value>
		</Parameter>
		<Parameter>
			<Name>RESTProxyList</Name> 
			<Value>REST_URL:egorest:REST_BASE_URL_1,CLOUD_REST_URL:cloudrest:REST_HOST_FACTORY_URL,SYM_REST_URL:symrest:SYMREST1</Value>
		</Parameter>
		<Parameter>
		   <Name>RestrictHostLogRetrieve</Name>
		   <!-- This parameter restricts retrieving logs for a host. -->
           <!-- Valid values are true or false. By default, this parameter is true. -->
		   <Value>true</Value>
		</Parameter>
		<Parameter>
			<Name>SaveResourcePlanHybridPolicyOwnershipUnbalanced</Name> 
			<!--Defines whether to save the hybrid scheduling policy that is not balanced. The value is either "true" or "false". -->
			<!--If set to "true", you can click either "OK" to continue or "Cancel" to not save the current changes. -->
			<!--If set to "false", an alert appears to check the unbalanced consumer field values, and you can click "OK" to not save the current changes. -->
			<Value>false</Value>
		</Parameter>
		<Parameter>
		   <Name>AllowlistRESTOrigin</Name>
           <!--This parameter allows you to define the specific host to access the REST server. -->
           <!--Define any number of hosts as required, separating multiple hosts by a semicolon (";"). -->
           <Value></Value>
        </Parameter>
        <Parameter>
		   <Name>checkOrigin</Name>
           <Value>false</Value>
        </Parameter>
        <Parameter>
           <Name>checkHeaderNamedHost</Name>
           <Value>false</Value>
       </Parameter>
       <Parameter>
           <Name>AllowlistOfHosts</Name>
           <Value></Value>
       </Parameter>
       <Parameter>
           <Name>allowEmptyHeaderNamedReferer</Name>
           <Value>true</Value>
       </Parameter>
  
  
	</Configuration>
</ParamConfs> 


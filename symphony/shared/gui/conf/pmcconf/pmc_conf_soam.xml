<?xml version="1.0" encoding="UTF-8"?> 
<!DOCTYPE ParamConfs SYSTEM "pmc_conf.dtd"> 
<ParamConfs> 
	<MainConsole TabStyle="top">
		<Module Name="SOAM" UserAclName="Main_SOAM" URL="/soamgui/getApplicationInfo.controller" OnlineHelpURL ="/doc/help/applications_control.html" LogoutURL="/soamgui/logout.controller" IconStyle="moduleTab-5">
			<ModuleItem Name="workload" UserAclName="Main_session" URL="/soamgui/getApplicationInfo.controller" HasConsumerTree="yes" OnlineHelpURL ="/doc/help/applications_control.html"/>
			<ModuleItem Name="configuration" UserAclName="Configuration" URL="/soamgui/getAppConfInfo.controller" HasConsumerTree="yes"/>
			<ModuleItem Name="ManageServicePackages" UserAclName="ServicePackageListView" URL="/soamgui/servicePackageInfoAction.controller" HasConsumerTree="yes"/>
		</Module>
	</MainConsole>
	<Dashboard>
		<Layout Name="WorkloadSummary" Module="SOAM" URL="/soamgui/soamsummary.controller" HasTitle="yes" HasPerformMin="no" HasResize="yes" HasMove="yes" HasAutoRefresh="yes" HasDefaultMin="no" Width="623" Height="263" Top="84" Left="544" MarginHeight="0" MarginWidth="0" FrameBorder="1" Scrolling="auto"  />
	</Dashboard>
	<Configuration>
		<Parameter>
			<!-- Task list cap, belong to SOAM-->
			<Name>TaskCap</Name>
			<Value>2000</Value>
		</Parameter>
		<Parameter>
			<!-- Session list cap, belong to SOAM-->
			<Name>SessionCap</Name>
			<Value>2000</Value>
		</Parameter>
		<Parameter>
			<!--  SOAM chart refresh interval, unit:second,it should be between 10 and 900, belong to SOAM -->
			<Name>SOAMChartInterval</Name>
			<Value>30</Value>
		</Parameter>
		<Parameter>
			<!--  timeout for sumbmitting a job (second). If this value is negative, fetching result will never time out.-->
			<Name>JobSubmitTimeOut</Name>
			<Value>-1</Value>
		</Parameter>
		<Parameter>
			 <!--  timeout for perform application/sessioin action with backend, unit:second,it should be bigger than 1 , belong to SOAM-->
			 <Name>PerformTimeOut</Name>
			 <Value>300</Value>
		 </Parameter>
		<Parameter>
			 <!--  timeout for query application/sessioin/task from backend, unit:second,it should be bigger than 1 , belong to SOAM-->
			 <Name>QueryTimeOut</Name>
			 <Value>60</Value>
		 </Parameter>
		 <Parameter>
			 <!--  retry limit for query application/sessioin/task/resource from backend, unit:account time,it should be bigger than 0 , belong to SOAM-->
			 <Name>QueryRetryLimit</Name>
			 <Value>3</Value>
		 </Parameter>
		 <Parameter>
			 <!--  timeout for register application from backend, unit:second,it should be bigger than 1 , belong to SOAM-->
			 <Name>RegAppTimeOut</Name>
			 <Value>180</Value>
		 </Parameter>
		<Parameter>
			<!--  Connect time limit when upload the file in Service Package Management. unit is seconds-->
			<Name>MaxResponseTimeOut</Name>    	
			<Value>600</Value>
		</Parameter>
		<Parameter>
			<!--  Time limit, in seconds, to retrieve a list of service packages while performing package management in the PMC-->
			<Name>MaxQueryTimeOut</Name>    	
			<Value>300</Value>
		</Parameter>
		<Parameter>
			<!-- Support version list, belong to SOAM-->
			<Name>supportVersionList</Name>
			<Value>3.1!3.2!4.0!4.1!5.0!5.1!5.2!6.1!6.1.1!7.1!7.1.1!7.1.2!7.2!7.2.1!7.3!7.3.1!7.3.2</Value>
		</Parameter>
		<Parameter>
			<!-- The value can be "yes" or "no". "no" means that only alphanumeric, '-', and '_' characters are allowed in the query. "yes" means there is no limitation, i.e., any characters can be used for the session name query. -->
	          	<Name>SessionNameCompatibility</Name>                       
			<Value>no</Value>
		</Parameter>
		<Parameter>
			<!-- The value can be "yes" or "no". "yes" means that '-' characters is allowed in the application name. "no" means '-' characters is not allowed in the application name. -->
	        <Name>EnableAppNameCheckDash</Name>                       
			<Value>no</Value>
		</Parameter>
	</Configuration>
</ParamConfs> 


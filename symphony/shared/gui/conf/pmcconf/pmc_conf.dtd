<!ELEMENT ParamConfs (MainConsole?,Dashboard?,Configuration?)>
<!ELEMENT MainConsole (Module*)> 
<!ATTLIST MainConsole TabStyle CDATA "top" >
<!ATTLIST Module Name CDATA #REQUIRED>
<!ATTLIST Module URL CDATA #REQUIRED>
<!ATTLIST Module UserAclName CDATA "">
<!ATTLIST Module LogoutURL CDATA "">
<!ATTLIST Module HasConsumerTree CDATA "no">
<!ATTLIST Module OnlineHelpURL CDATA "/platform/help/no_help.html">
<!ATTLIST Module IconStyle CDATA "moduleTab-0">
<!ATTLIST Module HasDefault CDATA "no">
<!ATTLIST Module TabIndex CDATA "0">
<!ELEMENT Module (ModuleItem*)>
<!ELEMENT ModuleItem EMPTY>
<!ATTLIST ModuleItem Name CDATA #REQUIRED>
<!ATTLIST ModuleItem URL CDATA #REQUIRED>
<!ATTLIST ModuleItem UserAclName CDATA "">
<!ATTLIST ModuleItem HasConsumerTree CDATA "no">
<!ATTLIST ModuleItem HasDefault CDATA "no">
<!ATTLIST ModuleItem LogoutURL CDATA "">
<!ATTLIST ModuleItem OnlineHelpURL CDATA "/platform/help/no_help.html">
<!ATTLIST ModuleItem IconStyle CDATA "moduleTab-0">
<!ATTLIST ModuleItem TabIndex CDATA "0">
<!ELEMENT Dashboard (Layout*)> 
<!ELEMENT Layout EMPTY>
<!ATTLIST Layout  Name CDATA #REQUIRED>
<!ATTLIST Layout  URL CDATA #REQUIRED>
<!ATTLIST Layout  Module CDATA #REQUIRED>
<!ATTLIST Layout  HasTitle CDATA "yes">
<!ATTLIST Layout  HasPerformMin CDATA "no">
<!ATTLIST Layout  HasResize CDATA "yes">
<!ATTLIST Layout  HasMove CDATA "yes">
<!ATTLIST Layout  HasAutoRefresh CDATA "yes">
<!ATTLIST Layout  HasDefaultMin CDATA "yes">
<!ATTLIST Layout  Width CDATA "440">
<!ATTLIST Layout  Height CDATA "500">
<!ATTLIST Layout  Top CDATA "0">
<!ATTLIST Layout  Left CDATA "0">
<!ATTLIST Layout  MarginHeight CDATA "0">
<!ATTLIST Layout  MarginWidth CDATA "0">
<!ATTLIST Layout  FrameBorder CDATA "1">
<!ATTLIST Layout  Scrolling CDATA "auto">
<!ELEMENT Configuration (Parameter*)>
<!ELEMENT Parameter (Name,Value)*>
<!ELEMENT Name (#PCDATA)>
<!ELEMENT Value (#PCDATA)>

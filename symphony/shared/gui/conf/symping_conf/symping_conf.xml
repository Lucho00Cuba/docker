<?xml version="1.0" encoding="UTF-8"?>
<symping>
	<configureFors>
		<configureFor name="COMPLETE_WORKLOAD_CYCLE" isDefault="true">
			<sessionTag></sessionTag>
			<taskTag></taskTag>
			<appName>symping7.3.2</appName>
			<sessionType>RecoverableAllHistoricalData</sessionType>
			<inputMsgSize>64|bytes</inputMsgSize>
			<outputMsgSize>128|bytes</outputMsgSize>
			<taskRunTime>50|milliseconds</taskRunTime>
			<commonDataSize>0|bytes</commonDataSize>
			<tasksPerRun>20</tasksPerRun>
			<consumeCpuCycles>false</consumeCpuCycles>
			<serviceLogOpt>false</serviceLogOpt>
			<enableDataCompression>false</enableDataCompression>
		</configureFor>
		<configureFor name="TASK_LATENCY" isDefault="false">
			<sessionTag></sessionTag>
			<taskTag></taskTag>
			<appName>symping7.3.2</appName>
			<sessionType>UnrecoverableNoHistoricalData</sessionType>
			<inputMsgSize>1024|bytes</inputMsgSize>
			<outputMsgSize>1024|bytes</outputMsgSize>
			<taskRunTime>0|milliseconds</taskRunTime>
			<commonDataSize>0|bytes</commonDataSize>
			<tasksPerRun>20</tasksPerRun>
			<consumeCpuCycles>false</consumeCpuCycles>
			<serviceLogOpt>false</serviceLogOpt>
			<enableDataCompression>false</enableDataCompression>
		</configureFor>
	</configureFors>
	<internalParams>
		<internalParam name="RefreshInterval">1</internalParam>
		<internalParam name="SeparateWinCount">4</internalParam>
		<internalParam name="MaxSympingInst">20</internalParam>
		<!-- The max value of MaxMemory can not exceed 2147483647 -->
		<internalParam name="MaxMemory">1000000000</internalParam>
	</internalParams>
</symping>

						highlightTabId="applicationLayout" aclResource="Main_session"
						aclPermission="1">
					</Node>
					<Node id="profile" label="@{pmc.tree.node.appProfiles.label}" icon=""
						status="" url="" layoutSourceId="symAppProfileLayout" tabGroup=""
						highlightTabId="symAppProfileLayout" aclResource="Configuration"
						aclPermission="1">
					</Node>
					<Node id="deprofile" label="@{pmc.tree.node.appProfiles.label}" icon=""

<?xml version="1.0" encoding="UTF-8"?>
<Tree xmlns="http://www.platform.com/gui/pmc_tree" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.platform.com/gui/pmc_tree pmc_tree.xsd">
	<TreeItem id="workloadTreeSource">
		<Nodes>
			<Node id="symphony" label="@{pmc.tree.node.symphony.label}" icon="" status="" url=""
				layoutSourceId="" tabGroup="" highlightTabId="" aclResource="Main_SOAM"
				aclPermission="1">
				<Nodes>
					<Node id="symphonyApp" label="@{pmc.tree.node.applications.label}" icon="" status=""

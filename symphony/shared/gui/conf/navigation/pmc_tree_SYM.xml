						tabGroup="" highlightTabId="" aclResource="ServicePackageListView"
						aclPermission="1">
					</Node>
					<Node id="desymServicePackage" label="@{pmc.tree.node.symServicePackage.label}"
						icon="" status="" url="" layoutSourceId="deservicePackageLayout"
						tabGroup="" highlightTabId="" aclResource="ServicePackageListView"
						aclPermission="1" helpGroupId="symServicePackages">
					</Node>
				</Nodes>
			</Node>
		</Nodes>
	</TreeItem>
</Tree>

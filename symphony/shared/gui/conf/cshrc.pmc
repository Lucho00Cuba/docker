#!/bin/csh
set GUI_VERSION=4.0
set PMC_TOP=@PMC_TOP@
setenv GUI_BINDIR ${PMC_TOP}/gui/${GUI_VERSION}/bin
setenv GUI_CONFDIR ${PMC_TOP}/gui/conf
setenv GUI_LIBDIR ${PMC_TOP}/gui/${GUI_VERSION}/lib/
set uname_m=`uname -m`
if ( "$uname_m" == "ppc64" ) then
    setenv GUI_LIBDIR ${PMC_TOP}/gui/${GUI_VERSION}/lib/
endif
echo "${PATH}" | grep ${GUI_BINDIR} >& /dev/null
if ( "$status" != "0" ) then
    setenv PATH "${GUI_BINDIR}:${PATH}"
endif

unsetenv GUI_BINDIR


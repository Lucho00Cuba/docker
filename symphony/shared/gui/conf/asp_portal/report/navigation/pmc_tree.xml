<?xml version="1.0" encoding="UTF-8"?>
<Tree xmlns="http://www.platform.com/gui/pmc_tree"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.platform.com/gui/pmc_tree pmc_tree.xsd">

	<TreeItem id="jobsTreeSource">
		<Nodes>
			<Node id="jobSubmission" label="@{pmc.tree.node.submissionforms.label}" icon="" status="" 
				url="/platform/application/toLoadAllForms.controller" 
				layoutSourceId="allJobs" tabGroup="" highlightTabId="" childUrl="/platform/application/loadAllFromsTreeNode.controller">
			</Node>
			<Node id="Jobs" label="@{pmc.tree.node.jobs.label}" icon="" status="" url=""
				layoutSourceId="jobs" tabGroup="" highlightTabId=""/>
			<Node id="JobFlows" label="@{pmc.tree.node.jobflows.label}" icon="" status=""
				 tabGroup="jobFlowsTabGroup" highlightTabId="jobFlowsSummary">
				<Nodes>
					<Node id="ppm_flowByUser" label="@{pmc.tree.node.jobflows.filter.user}" icon="" status=""
						layoutSourceId="ppm_flowByUser_layout" tabGroup="definitionTabGroup" highlightTabId=""  childUrl="../ppm/common/doGetFlowByUserNodes.controller"/>
					<Node id="ppm_flowByState" label="@{pmc.tree.node.jobflows.filter.state}" icon="" status=""
						layoutSourceId="ppm_flowByState_layout" tabGroup="instanceTabGroup" highlightTabId="" childUrl="../ppm/common/doGetFlowByStateNodes.controller"/>
				</Nodes>
			</Node>
			<Node id="JobData" label="@{pmc.tree.node.jobdata.label}" icon="" status="" url=""
				layoutSourceId="" tabGroup="" highlightTabId="">
				<Nodes>
					<Node id="JobDirectories" label="@{pmc.tree.node.jobdirectories.label}" icon="" status=""
						url="" layoutSourceId="jobdirectories" tabGroup="" highlightTabId=""/>
					<Node id="JobFlowDirectories" label="@{pmc.tree.node.jobflowdir.label}" status="" layoutSourceId="jobFlowDataLayout" />
					<Node id="JobDataExplore" label="@{pmc.tree.node.jobdataexplore.label}" icon="" status=""
						url="/platform/job/sharedDirectoryAction.controller"
						layoutSourceId="" tabGroup="" highlightTabId=""/>
				</Nodes>
			</Node>
			<Node id="JobConsoles" label="@{pmc.tree.node.jobconsoles.label}" icon="" status=""
				url="/platform/vnc/VNCList.controller" layoutSourceId="" tabGroup="" highlightTabId=""/>
		</Nodes>
	</TreeItem>

	<TreeItem id="resourceTreeSource">
		<Nodes>
			<Node id="DashboardNode" label="@{pmc.tree.node.dashboard.label}" icon="" status=""  
				 url="/platform/dashboard/toResDashboard.controller" aclResource="System" aclPermission="3">
			</Node>
			<Node id="Hosts" label="@{pmc.tree.node.hosts.label}" icon="" status=""  
				url="" layoutSourceId="hostLayout" tabGroup="hostTabGroup" highlightTabId=""
				aclResource="All Hosts" aclPermission="1">
			</Node>
			<Node id="ApplicationTemplates" label="@{pmc.tree.node.applicationtemplates.label}" status="" 
						url="/platform/application/toLoadAppTemplates.controller" 
						aclResource="APP_TEMPLATE_-ALL_TEMPLATES_NODE" aclPermission="1"/>
			<Node id="licenseUsages" label="@{pmc.tree.node.licenses.label}" url="../pls/toLicenseSummary.controller"
				aclResource="License Scheduler" aclPermission="1" childUrl="../pls/toLicenseTree.controller">
			</Node>
		</Nodes>
	</TreeItem>
	
	<TreeItem id="settingsTreeSource">
		<Nodes>
			<Node id="users" label="@{pmc.tree.node.settings.users.label}" url="" aclResource="System" aclPermission="3" layoutSourceId="userGroupLayout">
				<Nodes>
					<Node id="userGroups" label="@{pmc.tree.node.settings.userGroups.label}" layoutSourceId="userGroupLayout"/>
					<Node id="userRoles" label="@{pmc.tree.node.settings.userRoles.label}" layoutSourceId="userRoleLayout" />
				</Nodes>
			</Node>
			<Node id="logo_and_color" label="@{pmc.tree.node.logo_color.label}" status="" 
						url="/platform/framework/portal/toConfigureAppearance.controller" aclResource="System" aclPermission="3" />
			<Node id="pageSettings" label="@{pmc.tree.node.pageSettings.label}" status=""
						url="/platform/pac/configurations/toJobPageSetting.controller" aclResource="System" aclPermission="3" />
			
		</Nodes>
	</TreeItem>

	<TreeItem id="reportsTreeSource">
		<Nodes>
			<Node id="Resource_Usage" label="@{pmc.tree.node.perfreports.resourceusage.label}" status="" 
                                url="/report/reportListingAction.controller?folder=ResourceUsage&amp;reportType=2"/>
			<Node id="CPU_Monitor" label="@{pmc.tree.node.perfreports.cpumonitor.label}" status="" 
                                url="/report/reportListingAction.controller?folder=JobSlotUsage&amp;reportType=2"/>
			<Node id="Done_Job_Monitor" label="@{pmc.tree.node.perfreports.donejobmonitor.label}" status="" 
                                url="/report/reportListingAction.controller?folder=DoneJobStatus&amp;reportType=2"/>
                        <Node id="Run_Job_Monitor" label="@{pmc.tree.node.perfreports.runjobmonitor.label}" status="" 
                                url="/report/reportListingAction.controller?folder=RunJobStatus&amp;reportType=2"/>
			<Node id="License_Usage" label="@{pmc.tree.node.perfreports.licenseusage.label}" status="" 
                                url="/report/reportListingAction.controller?folder=LicenseUsage&amp;reportType=2"/>
                        <Node id="SAN_Usage" label="@{pmc.tree.node.perfreports.sanusage.label}" status="" 
                                url="/report/reportListingAction.controller?folder=SANUsage&amp;reportType=2"/>
			<Node id="Queue_Accounting" label="@{pmc.tree.node.perfreports.queueaccounting.label}" status="" 
                                url="/report/reportListingAction.controller?folder=QueueAccounting&amp;reportType=2"/>
		</Nodes>
	</TreeItem>

	<TreeItem id="reportInstanceTreeSource">
                <Nodes>
                        <Node id="Resource_Usage_Cluster" label="@{pmc.tree.node.perfreports.instance.resourceusage.cluster.label}" status="" 
                                url="/report/reportInstanceManageAction.controller?directory=<reportInstanceDirectory>&amp;reportType=2&amp;dispatch=reportInstanceLIST"/>
			<Node id="SAN_Usage_Usage" label="@{pmc.tree.node.perfreports.instance.sanusage.label}" status="" 
                                url="/report/reportInstanceManageAction.controller?directory=<reportInstanceDirectory>&amp;reportType=2&amp;dispatch=reportInstanceLIST"/>
                </Nodes>
        </TreeItem>
</Tree>

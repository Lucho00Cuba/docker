<?xml version="1.0" encoding="UTF-8"?>
<Navigations xmlns="http://www.platform.com/gui/pmc_navigation_tab"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.platform.com/gui/pmc_navigation_tab pmc_navigation_tab.xsd">
	
	<Navigation id="jobs" label="@{pmc.navigation.tab.jobs.label}" status="display" icon="images/tabicons/workloadMgmt.gif"
		layoutSourceId="" tabGroup="" highlightTabId="" url="">
		<Handler id="testTrigger" type="tree">
			<Datasource>jobsTreeSource</Datasource>
			<SelectNodeId>Jobs</SelectNodeId>
		</Handler>
	</Navigation>
	
	<Navigation id="resource" label="@{pmc.navigation.tab.resources.label}" status="display" icon="images/tabicons/pcmcluster.gif"
		layoutSourceId="" tabGroup="" highlightTabId="" url="">
		<Handler id="resourceTrigger" type="tree">
			<Datasource>resourceTreeSource</Datasource>
			<SelectNodeId></SelectNodeId>
		</Handler>
	</Navigation>
	
	<Navigation id="settings" label="@{pmc.navigation.tab.settings.label}" status="display" icon="images/tabicons/pmcusers.gif" >
		<Handler id="configurationsTrigger" type="tree">
			<Datasource>settingsTreeSource</Datasource>
			<SelectNodeId>userGroups</SelectNodeId>
		</Handler>
	</Navigation>
	
	<Navigation id="reports" label="@{pmc.navigation.tab.perfreports.label}" status="display" icon="images/tabicons/report3d.gif"
		layoutSourceId="" tabGroup="" highlightTabId="" url="/report/reportListingAction.controller?folder=ResourceUsage&amp;reportType=2" aclResource="All Reports" aclPermission="1">
		<Handler id="reportsTrigger" type="tree">
			<Datasource>reportsTreeSource</Datasource>
			<SelectNodeId></SelectNodeId>
		</Handler>
	</Navigation>

	<Navigation id="reportInstance" label="@{pmc.navigation.tab.perfreports.instance.label}" status="display" icon="images/tabicons/report3d.gif"
                layoutSourceId="" tabGroup="" highlightTabId="" url="/report/reportInstanceManageAction.controller?directory=<reportInstanceDirectory>&amp;reportType=2&amp;dispatch=reportInstanceLIST" aclResource="All Reports" aclPermission="1">
                <Handler id="reportInstanceTrigger" type="tree">
                        <Datasource>reportInstanceTreeSource</Datasource>
                        <SelectNodeId></SelectNodeId>
                </Handler>
        </Navigation>
</Navigations>

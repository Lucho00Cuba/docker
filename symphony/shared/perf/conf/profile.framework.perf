#!/bin/bash
if [ -z "${LICENSE_FILE}"  ]; then
	LICENSE_FILE=`grep '^[      ]*LICENSE_FILE' ${PERF_CONFDIR}/perf.conf | sed -e "s/^.*=//g" | sed -e "s/\"//g"`
fi

if [ -z "${LICENSE_FILE}"  ]; then
	LICENSE_FILE=${PERF_CONFDIR}/license.dat
fi

if [ -z "${LICENSE_FILE}"  ]; then
	LICENSE_FILE=
fi

export LICENSE_FILE
